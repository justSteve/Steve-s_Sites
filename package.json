{
  "name": "wayback-archive-toolkit",
  "version": "1.0.0",
  "description": "Multi-domain Wayback Machine archive toolkit with intelligent snapshot selection",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "cdx-analyzer": "node dist/cli/cdx-analyzer.js",
    "generate-timeline": "node dist/cli/generate-timeline.js",
    "select-snapshots": "node dist/cli/select-snapshots.js",
    "wayback-crawler": "node dist/cli/wayback-crawler.js"
  },
  "keywords": [
    "wayback",
    "archive",
    "web-scraping",
    "internet-archive"
  ],
  "author": "Steve Yegge",
  "license": "MIT",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "better-sqlite3": "^9.2.2",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "winston": "^3.11.0",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "optionalDependencies": {
    "vue": "^3.4.0",
    "vite": "^5.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "pinia": "^2.1.7"
  }
}
