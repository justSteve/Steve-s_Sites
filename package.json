{
  "name": "wayback-archive-toolkit",
  "version": "1.0.0",
  "description": "Multi-domain Wayback Machine archive toolkit with intelligent snapshot selection",
  "main": "dist/index.js",
  "scripts": {
    "build": "bun run tsc",
    "build:frontend": "bun run vite build",
    "watch": "bun run tsc --watch",
    "dev": "bun run vite",
    "api": "bun run dist/api/index.js",
    "menu": "bun run dist/cli/menu.js",
    "start": "bun run menu",
    "test": "bun run jest",
    "test:watch": "bun run jest --watch",
    "test:coverage": "bun run jest --coverage",
    "lint": "bun run eslint src/**/*.ts",
    "format": "bun run prettier --write src/**/*.ts",
    "cdx-analyzer": "bun run dist/cli/cdx-analyzer.js",
    "crawler": "bun run dist/cli/crawler.js",
    "selector": "bun run dist/cli/selector.js",
    "generator": "bun run dist/cli/generator.js",
    "typecheck": "bun run tsc --noEmit"
  },
  "keywords": [
    "wayback",
    "archive",
    "web-scraping",
    "internet-archive"
  ],
  "author": "Steve Yegge",
  "license": "MIT",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^29.5.11",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^20.10.6",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "vite": "^7.1.10"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@myorg/api-server": "file:../../packages/api-server",
    "@myorg/dashboard-ui": "file:../../packages/dashboard-ui",
    "axios": "^1.6.5",
    "better-sqlite3": "^9.2.2",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "inquirer": "^9.3.8",
    "mime-types": "^3.0.1",
    "open": "^10.2.0",
    "winston": "^3.11.0"
  },
  "optionalDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "pinia": "^2.1.7",
    "vite": "^5.0.0",
    "vue": "^3.4.0"
  }
}
